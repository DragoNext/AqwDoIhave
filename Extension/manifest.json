{
	"manifest_version":3,
	"name":"AqwDoIhave",
	
	"description":"Compares items to items that you imported for aqwiki page",
	"version":"1.0",
	"options_page": "options.html",
	"icons": {
		"16":"/images/16.png",
		"48":"/images/48.png",
		"128":"/images/128.png"
	}, 
	  "web_accessible_resources": [ {
      "resources": [ "js/ProcessAcountItems.js", "js/ProcessWikiItem.js", "themes/aqw.css","themes/dark.css","images/BannerMain.png","images/in_bank.png","images/in_inventory.png","images/price_icon.png"] ,
      "matches": ["<all_urls>"] }
	],
	"content_scripts": [{
		"matches": ["http://aqwwiki.wikidot.com/*","https://account.aq.com/AQW/Inventory"],
		"run_at": "document_start",
		"js": ["js/Themes.js","js/ProcessAcountItems.js","js/ProcessWikiItem.js","js/main.js"]
	}],
	
	"permissions": [
        "storage",
		 "contextMenus"
    ]
}